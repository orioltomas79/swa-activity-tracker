  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.13.7",
    "@mui/material": "^5.13.7",
    "@mui/x-date-pickers": "^6.10.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.57",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "dayjs": "^1.11.9",
    "formik": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/chance": "^1.1.6",
    "builder-pattern": "^2.2.0",
    "chance": "^1.1.11",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nswag": "^13.18.2",
    "prettier": "^2.8.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "swa:start": "swa start http://localhost:3000",
    "format": "prettier --write src/**/*.{ts,tsx,scss,css,json}",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "api:generate": "npm run api:generate:nswag && npm run api:postgenerate",
    "api:generate:nswag": "nswag openapi2tsclient /runtime:Net60 /typeScriptVersion:4.9 /input:http://localhost:7071/api/swagger.json /output:src/api/apiClient.g.nswag.ts /template:fetch /operationGenerationMode:MultipleClientsFromFirstTagAndOperationName /typeStyle:Interface /nullValue:null /dateTimeType:string",
    "api:postgenerate": "prettier --write src/api/apiClient.g.nswag.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
