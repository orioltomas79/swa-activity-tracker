{
  "name": "my-activity-tracker-app",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "func:start": "func host start --script-root ../activity-tracker-api",
    "swa:start": "swa start http://localhost:3000",
    "api:generate": "npm run api:generate:nswag && npm run api:postgenerate",
    "api:generate:nswag": "nswag openapi2tsclient /runtime:Net60 /typeScriptVersion:4.9 /input:http://localhost:7071/api/swagger.json /output:src/api/apiClient.g.nswag.ts /template:axios /operationGenerationMode:MultipleClientsFromPathSegments /typeStyle:Interface /nullValue:null /dateTimeType:string",
    "api:postgenerate": "prettier --write src/api/apiClient.g.nswag.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.13.7",
    "@mui/material": "^5.13.7",
    "@mui/x-date-pickers": "^6.10.0",
    "axios": "^1.3.4",
    "dayjs": "^1.11.9",
    "formik": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "nswag": "^13.18.2",
    "prettier": "^2.8.7",
    "typescript": "^5.0.2",
    "vite": "^4.4.0"
  }
}
